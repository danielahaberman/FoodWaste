{
  "name": "food-hero",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:web": "vite",
    "wait:api": "node scripts/wait-for-api.js",
    "dev:web:wait": "npm run wait:api && npm run dev:web",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "npm --prefix server run start",
    "dev:api": "node scripts/dev-api.js",
    "dev:all": "concurrently -n api,web -c cyan,green \"npm:dev:api\" \"npm:dev:web:wait\"",
    "devhost:all": "node scripts/get-local-ip.js && concurrently -n api,web -c cyan,green \"npm:dev:api\" \"npm:dev:web:wait\"",
    "live": "vite --mode live",
    "start": "npm run dev:all",
    "local-ip": "node scripts/get-local-ip.js",
    "update-version": "node scripts/update-version.js",
    "git-push": "node scripts/git-push.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@mui/x-date-pickers": "^8.0.0",
    "axios": "^1.8.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "pg": "^8.16.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-ios-pwa-prompt": "^2.0.6",
    "react-qr-barcode-scanner": "^2.1.20",
    "react-router-dom": "^7.1.3",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "react-swipeable-views": "^0.14.0",
    "react-use-navigate": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "vite": "^6.2.0"
  }
}
